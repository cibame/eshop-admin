{
  "name": "eshop-admin",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "fixtures:dev": "npx fixtures -s -d test/fixtures",
    "fixtures:prod": "npx fixtures -s -d test/fixtures -c ormconfig.prod.yml",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --color --run-in-band",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start-admin:dev": "nest start --watch --config=nest-cli-admin.json",
    "start-admin:prod": "node dist/main-admin",
    "build-admin": "nest build --config=nest-cli-admin.json"
  },
  "dependencies": {
    "@admin-bro/express": "3.0.1",
    "@admin-bro/nestjs": "1.0.0",
    "@admin-bro/typeorm": "^1.4.0",
    "@admin-bro/design-system": "1.7.0",
    "@nestjs-modules/mailer": "1.5.1",
    "@nestjs/common": "~7.5.5",
    "@nestjs/config": "~0.6.1",
    "@nestjs/core": "~7.5.5",
    "@nestjs/platform-express": "~7.5.5",
    "@nestjs/swagger": "~4.7.5",
    "@nestjs/typeorm": "~7.1.5",
    "admin-bro": "~3.3.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "ejs": "3.1.5",
    "handlebars": "4.7.6",
    "express-formidable": "1.2.0",
    "express-session": "1.17.1",
    "express": "4.17.1",
    "pg": "^8.5.1",
    "pug": "3.0.0",
    "reflect-metadata": "^0.1.13",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.29",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "3.4.3",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint": "7.7.0",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm-fixtures-cli": "^1.8.1",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "verbose": true,
    "bail": true
  }
}
